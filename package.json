{
  "name": "typescript-library-starter-master",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "author": "David Alsh <alshdavid@gmail.com>",
  "scripts": {
    "build": "make",
    "lint": "make lint",
    "precommit": "lint-staged",
    "start": "make dev",
    "test": "make test",
    "test:watch": "make test-watch"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "prettier": "^1.19.1",
    "rollup": "^1.27.14",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.4"
  }
}